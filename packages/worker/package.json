{
  "name": "@auth/worker",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "main": "dist/worker.service.js",
  "bin": {
    "worker": "./dist/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/worker.service.d.ts",
      "import": "./dist/worker.service.js",
      "default": "./dist/worker.service.js"
    },
    "./worker.service": {
      "types": "./dist/worker.service.d.ts",
      "import": "./dist/worker.service.js",
      "default": "./dist/worker.service.js"
    },
    "./consumers/email": {
      "types": "./dist/consumers/email.consumer.d.ts",
      "import": "./dist/consumers/email.consumer.js",
      "default": "./dist/consumers/email.consumer.js"
    }
  },
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest run --coverage",
    "build": "tsc",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@auth/app-bootstrap": "workspace:*",
    "@auth/config": "workspace:*",
    "@auth/contracts": "workspace:*",
    "@auth/core": "workspace:*",
    "@auth/database": "workspace:*",
    "@auth/email": "workspace:*",
    "@auth/queues": "workspace:*",
    "@auth/utils": "workspace:*",
    "@opentelemetry/api": "^1.9.0",
    "@sentry/node": "^10.29.0",
    "@sentry/profiling-node": "^10.29.0",
    "bullmq": "^5.65.1",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/node": "^24.10.1"
  }
}